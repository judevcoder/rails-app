- if @entity.key.present?
  .links
    = link_to "Basic info", entities_tenancy_in_common_basic_info_path(@entity.key), class: "btn btn-sm btn-default margin-sm-left pull-left entity-page-xhr #{params[:action] == 'basic_info' ? 'active' : ''}"
    = link_to "Tenants in Common", entities_tenancy_in_common_tenants_in_common_path(@entity.key), remote: true, class: "btn btn-sm btn-default margin-sm-left pull-left entity-page-xhr #{params[:action] == 'tenants_in_common' ? 'active' : ''}"
    = link_to "+", entities_tenancy_in_common_tenant_in_common_path(@entity.key), remote: true, class: "btn btn-sm btn-default margin-sm-left pull-left entity-page-xhr #{params[:action] == 'tenant_in_common' ? 'active' : ''}"
    = link_to raw("<span class='glyphicon glyphicon-cog'></span>"), "#", class: "btn btn-sm btn-default margin-sm-left pull-left"

